    <Button
        android:id="@+id/start_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/model_btn"
        android:text="m download"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/start_btn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/jni64_btn"
        android:text="64 download"
        app:layout_constraintTop_toBottomOf="@+id/model_btn"
        app:layout_constraintLeft_toRightOf="@+id/start_btn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/jni32_btn"
        android:text="32 download"
        app:layout_constraintTop_toBottomOf="@+id/jni64_btn"
        app:layout_constraintLeft_toRightOf="@+id/start_btn" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/run_constructor_btn"
        android:text="run const"
        app:layout_constraintTop_toTopOf="parent"
